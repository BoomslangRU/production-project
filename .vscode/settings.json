{
   "i18n-ally.localesPaths": [
      "public/locales",
      "src/shared/config/i18n"
   ],
   "react-i18n.i18nPaths": "public\\locales,src\\shared\\config\\i18n",
   "cSpell.words": [
      "languagedetector",
      "linebreak",
      "stylelint",
      "svgr",
      "testid",
      "Главная",
      "найдена",
      "страница"
   ],
   "stylelint.enable": true,
   "css.validate": true,
   "scss.validate": true,
   "stylelint.validate": [
      "css",
      "scss"
   ],
   "editor.minimap.scale": 1,
   "editor.minimap.size": "proportional",
   "editor.insertSpaces": true,
   "editor.tabSize": 3,
   "editor.cursorStyle": "block"
}